{
  "name": "vue2-leaflet",
  "version": "2.4.3",
  "description": "Vue2 leaflet library",
  "author": "Mickael Bouchaud",
  "scripts": {
    "serve": "npm run build -- -w",
    "build": "rollup -c --environment NODE_ENV:production",
    "prepare": "npm run lint && npm run test && npm run build",
    "postversion": "git push --follow-tags",
    "lint": "eslint --ext .js,.vue --ignore-path .eslintignore .",
    "test": "jest",
    "test:w": "jest --watch",
    "docs:serve": "docsify serve ./docs -p 4000 -o"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "babel-core": "^7.0.0-bridge.0",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "leaflet": "^1.6.0",
    "lint-staged": "^10.0.7",
    "prettier": "1.19.1",
    "rollup": "^1.31.0",
    "rollup-copy-plugin": "^0.1.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-vue": "^5.1.6",
    "typescript": "^3.7.5",
    "vue": "^2.6.11",
    "vue-jest": "3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "files": [
    "dist/",
    "src/",
    "types/*.d.ts"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "vue",
    "leaflet"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ],
    "*.vue": [
      "eslint",
      "git add"
    ]
  },
  "main": "dist/vue2-leaflet.cjs.js",
  "unpkg": "dist/vue2-leaflet.min.js",
  "module": "dist/vue2-leaflet.es.js",
  "sideEffects": false,
  "peerDependencies": {
    "@types/leaflet": "^1.5.5",
    "leaflet": "^1.3.4",
    "vue": "^2.5.17"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/KoRiGaN/Vue2Leaflet.git"
  },
  "typings": "types/index.d.ts"
}
